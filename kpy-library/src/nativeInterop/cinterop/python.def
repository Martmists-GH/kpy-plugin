headers = Python.h
package = python
compilerOpts = -I"/usr/include/python3.10"
linkerOpts = -L"/usr/lib" -l python3

---

struct KtPyObject {
    PyObject base;
    void* ktObject;
};

// Wrapper func for _PyUnicode_AsString macro
char* PyUnicode_AsString(PyObject* obj) {
    return _PyUnicode_AsString(obj);
}
